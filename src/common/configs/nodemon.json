{
    "restartable": "rs",
    "legacy-watch": true,
    "cleanly": true,
    "ignore": [
        "**/*.test.ts"
    ],
    "verbose": true,
    "events": {},
    "execMap": {
        "": "node",
        "js": "node --harmony",
        "ts": "ts-node -r tsconfig-paths/register"
    },
    "exec": "ts-node -r tsconfig-paths/register ./src",
    "watch": [
        "src/**/*.{ts,json}"
    ],
    "env": {},
    "ext": "ts js json"
}